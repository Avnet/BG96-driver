{
    "name": "BG96-library",
    "config": {
       "bg96_reset": {
           "help": "RESET pin for the BG96",
           "value": "D7"
       },
       "bg96-wake": {
           "help": "Wake pin to BG96",
           "value": "D11"
       },
       "bg96-pwrkey": {
           "help": "pwr key input to BG96",
           "value": "D10"
       },
       "bg96-tx": {
           "help": "TX pin for serial connection to BG96",
           "value": "D8"
       },
       "bg96-rx": {
           "help": "RX pin for serial connection to BG96",
           "value": "D2"
       }
    }
}


